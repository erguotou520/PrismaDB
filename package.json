{
  "name": "PrismaDB",
  "version": "0.0.1",
  "private": true,
  "license": "GPL-3.0-or-later",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "eslint": "eslint . --ext js,jsx,ts,tsx,json --quiet --fix",
    "postinstall": "husky install",
    "lint:all": "npm run eslint && npm run stylelint && npm run prettier",
    "prettier": "prettier \"*.!(js|jsx|ts|tsx|css|less|styl|scss|sass)\" --write",
    "preview": "vite preview",
    "stylint": "stylelint \"src/**/*.{css,less,scss,sass,styl}\" --fix",
    "tauri": "tauri"
  },
  "prettier": "@doremijs/prettier-config",
  "stylelint": {
    "extends": "@doremijs/stylelint-config"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@mantine/core": "^6.0.19",
    "@mantine/dates": "^6.0.19",
    "@mantine/form": "^6.0.19",
    "@mantine/hooks": "^6.0.19",
    "@mantine/modals": "^6.0.19",
    "@tabler/icons-react": "^2.30.0",
    "@tauri-apps/api": "^1.4.0",
    "dayjs": "^1.11.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.4.4",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.15.0",
    "unocss": "^0.55.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@doremijs/prettier-config": "^0.1.3",
    "@doremijs/stylelint-config": "^0.1.8",
    "@tauri-apps/cli": "^1.4.0",
    "@types/node": "^20.4.10",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@unocss/preset-wind": "^0.55.0",
    "@unocss/transformer-directives": "^0.55.0",
    "@unocss/transformer-variant-group": "^0.55.0",
    "@vitejs/plugin-react": "^4.0.4",
    "babel-plugin-formatjs": "^10.5.3",
    "devmoji": "^2.3.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "prettier": "^3.0.1",
    "stylelint": "^15.10.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-pages": "^0.31.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "stylelint": "15"
      }
    },
    "allowedDeprecatedVersions": {
      "source-map-resolve": "0.5.3",
      "resolve-url": "0.2.1",
      "urix": "0.1.0",
      "source-map-url": "0.4.1",
      "@types/vfile-message": "2"
    }
  }
}
